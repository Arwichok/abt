{
    "name": "abt",
    "description": "Aiogram Bot Template",
    "runtime": "python3.9",
    "env": [
        {
            "key": "TELEGRAM_BOT_TOKEN",
            "description": "Token to telegram bot from https://t.me/botfather",
            "required": true
        },
        {
            "key": "TELEGRAM_SECRET",
            "description": "Secret data to connect with telegram bot api",
            "required": true,
            "default": "some_secret_data"
        }
    ]
}